    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: post_gen",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}/protos",
            "program": "post_gen.py",
            "args": [
                "--python_out=../src/terraform_provider_python/tfplugin",
                "--pyi_out=../src/terraform_provider_python/tfplugin",
                "--grpc_python_out=../src/terraform_provider_python/tfplugin",
                "--proto_file=./tfplugin.proto",
            ]   
        },
    ]
}